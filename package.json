  "main": "client.js",
  "scripts": {
    "start:client": "webpack --watch --progress --env port=3000",
    "start:server": "cd ./server && npm run start && cd ..",
    "start:backend": "node ./json-server/index.js --port 8123",
    "start:ssr": "npm run build:frontend && npm run start:server",
    "build": "npm run build:frontend && npm run build:server",
    "build:frontend": "npm run build:dev",
    "build:server": "cd server && npm install && npm run build:dev && cd ..",
    "build:prod": "webpack --env mode=production platform=client analyze=0 PROFILE_MF_URL=http://localhost:3006",
    "build:dev": "webpack --env mode=development platform=client analyze=0",
    "analyze:prod": "cross-env analyze=1 webpack --env mode=production",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache --fix",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "unit": "npx jest --config config/jest/jest.config.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint"
  },
